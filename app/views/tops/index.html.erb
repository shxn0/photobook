<div class="container">
  <div class="row col-md-8 col-md-offset-2">
    <% if notice.nil? ==false %>
      <div class="alert alert-success" role="alert">
        <p class="flash"><%= notice %><%= alert %></p>
      </div>
    <% end %>
    <h1>Top#index</h1>
    <div class= "wrap col-md-4 col-md-offset-4 col-sm-10 col-sm-offset-1" %>
      <%= link_to "wall", walls_path, class:"btn btn-default btn-block" %>
    </div>
  </div>
</div>
